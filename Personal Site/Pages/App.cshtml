@page "{name}"
@model AppModel
@{
    ViewData["Title"] = Model.Name;
    ViewData["SubURL"] = "app/" + Model.Name;
    ViewData["Description"] = Model.Description;
    ViewData["Thumbnail"] = Model.ThumbnailPath;
    ViewData["HideMenu"] = true;
}

@section Head {
    <bundle-js name="three" versioned />
    <bundle-js name="@(Model.Name + "-app")" versioned />
    <style>
        body {
            background: fixed center/cover url(/@Model.ThumbnailPath);
        }
    </style>
}

    @Html.AntiForgeryToken()