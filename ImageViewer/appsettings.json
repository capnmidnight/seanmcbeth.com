{
    "Version": "0.0.1",
    "AllowedHosts": "127.0.0.1",
    "Kestrel": {
        "Endpoints": {
            "HTTP": {
                "Url": "http://127.0.0.1:0"
            }
        }
    },
    "ContentTypes": {
        ".glb": "model/gltf-binary",
        ".gltf": "model/gltf-json",
        ".vtt": "text/vtt"
    },
    "DefaultFiles": [
        "index.html"
    ],
    "AppShell": {
        "SplashScreenPath": "splash.html",
        "Window": {
            "Title": "Image Viewer",
            "Fullscreen": true,
            "Borderless": true,
            "Maximized": true
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.Hosting.Lifetime": "Warning"
        }
    }
}